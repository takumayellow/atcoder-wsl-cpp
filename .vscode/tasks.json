{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "C: Run tests (oj)",
			"type": "shell",
			"command": "powershell",
			"args": [
				"-NoProfile",
				"-File",
				"${workspaceFolder}\\.vscode\\run-oj.ps1",
				"${fileDirname}"
			],
			"group": {
				"kind": "test",
				"isDefault": true
			},
			"problemMatcher": [],
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": true,
				"panel": "shared",
				"clear": true
			}
		},
		{
			"label": "Patch AtCoder User-Agent",
			"type": "shell",
			"command": "wsl python3 /mnt/c/Users/takum/Documents/atcoder/atcoder-wsl-cpp/patch_atcoder_ua.py",
			"isBackground": false,
			"group": "test"
		},
		{
			"label": "S: Submit code (acsub)",
			"type": "shell",
			"command": "wsl",
			"args": [
				"acsub"
			],
			"isBackground": false,
			"group": "none"
		}
	]
}