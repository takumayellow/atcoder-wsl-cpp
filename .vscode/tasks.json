{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "C: Run tests (oj)",
      "type": "shell",
      "command": "powershell",
      "args": [
        "-NoProfile",
        "-File",
        "${workspaceFolder}\\.vscode\\run-oj.ps1",
        "${fileDirname}"
      ],
      "group": {
        "kind": "test",
        "isDefault": true
      },
      "problemMatcher": [],
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": true,
        "panel": "shared",
        "clear": true
      }
    }
  ]
}
